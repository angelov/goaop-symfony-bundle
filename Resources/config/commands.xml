<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="goaop.command.warmup" public="true" class="Go\Symfony\GoAopBundle\Command\CacheWarmupCommand">
            <argument type="service" id="goaop.aspect.kernel" />
            <tag name="console.command" />
        </service>

        <service id="goaop.command.debug_advisor" public="true" class="Go\Symfony\GoAopBundle\Command\DebugAdvisorCommand">
            <argument type="service" id="goaop.aspect.kernel" />
            <tag name="console.command" />
        </service>

        <service id="goaop.command.debug_aspect" public="true" class="Go\Symfony\GoAopBundle\Command\DebugAspectCommand">
            <argument type="service" id="goaop.aspect.kernel" />
            <tag name="console.command" />
        </service>

    </services>

</container>
